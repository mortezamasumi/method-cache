{
    "name": "mortezamasumi/method-cache",
    "description": "Cache the result of any method.",
    "keywords": [
        "php",
        "laravel",
        "caching"
    ],
    "homepage": "https://github.com/mortezamasumi/method-cache",
    "license": "MIT",
    "authors": [
        {
            "name": "Morteza Masumi",
            "email": "morteza.masumi@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "mortezamasumi/method-overrider": "dev-main"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^9.6",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Mortezamasumi\\MethodCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mortezamasumi\\MethodCache\\Tests\\": "tests/"
        }
    },
    "scripts": {},
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mortezamasumi\\MethodCache\\MethodCacheServiceProvider"
            ],
            "aliases": {
                "FilamentBase": "Mortezamasumi\\MethodCache\\Facades\\MethodCache"
            }
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mortezamasumi/method-overrider"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
